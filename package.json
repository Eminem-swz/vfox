{
  "name": "vfox",
  "version": "2.0.0-beta.14",
  "description": "Vue components include form, toast, modal, etc.",
  "main": "./dist/index.js",
  "module": "./es/index.js",
  "directories": {
    "doc": "./examples/docs"
  },
  "scripts": {
    "test:unit": "jest --detectOpenHandles",
    "build": "webpack --config=./build/webpack.config.js",
    "build:svg": "webpack --config=./build/webpack.svg.config.js",
    "lint": "eslint ./src/**/*.js",
    "build:css": "gulp build --gulpfile build/gulpfile.js",
    "build:component": "node ./build/bincomp.js",
    "build:es": "gulp build --gulpfile build/gulpfile.js & rollup -c ./build/build.full.js & gulp dts --gulpfile build/gulpfile.js & node ./build/bincomp.js",
    "build:full": "rollup -c ./build/build.full.js",
    "build:mainfest": "node build/build.mainfest.js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godxiaoji/vfox.git"
  },
  "keywords": [
    "vue",
    "components",
    "API",
    "UI"
  ],
  "author": "Travis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godxiaoji/vfox/issues"
  },
  "files": [
    "dist",
    "es",
    "src"
  ],
  "homepage": "https://github.com/godxiaoji/vfox#readme",
  "dependencies": {
    "async-validator": "^3.4.0",
    "dayjs": "^1.10.4",
    "qrcode": "^1.4.4",
    "safe-area-insets": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime-corejs3": "^7.12.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/qrcode": "^1.4.0",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-rc.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^3.4.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-loader": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^5.3.0",
    "prettier": "^1.19.1",
    "rollup": "^2.39.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "semantic-release": "^17.3.3",
    "svg-sprite-loader": "^5.0.0",
    "through2": "^4.0.2",
    "ts-jest": "^26.5.3",
    "typescript": "^4.1.5",
    "vue": "^3.0.11",
    "vue-jest": "^5.0.0-alpha.8",
    "vue-loader": "^16.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": [
    "src/**/style/*",
    "src/**/lib/*",
    "*.css",
    "*.scss",
    "*.vue"
  ]
}
