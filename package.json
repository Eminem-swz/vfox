{
  "name": "vfox",
  "version": "2.0.4",
  "description": "Vue components include form, toast, modal, etc.",
  "main": "dist/index.js",
  "module": "es/index.js",
  "style": "es/style/index.css",
  "typings": "es/index.d.ts",
  "directories": {
    "doc": "./examples/docs"
  },
  "scripts": {
    "test:unit": "jest --detectOpenHandles",
    "build:umd": "rollup -c ./build/build.umd.js",
    "build:svg": "rollup -c ./build/rollup.svg.js",
    "lint": "eslint ./src/**/*.ts",
    "build:css": "gulp build --gulpfile build/gulpfile.js",
    "build:component": "node ./build/bincomp.js",
    "build:full": "node ./build/build.declaration.js",
    "build:manifest": "node build/build.manifest.js",
    "semantic-release": "semantic-release",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "demo:dev": "vite",
    "demo:build": "vue-tsc --noEmit && vite build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/godxiaoji/vfox.git"
  },
  "keywords": [
    "vue",
    "components",
    "API",
    "UI"
  ],
  "author": "Travis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/godxiaoji/vfox/issues"
  },
  "files": [
    "dist",
    "es"
  ],
  "homepage": "https://github.com/godxiaoji/vfox#readme",
  "dependencies": {
    "async-validator": "^3.4.0",
    "dayjs": "^1.10.4",
    "safe-area-insets": "^1.4.1",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime-corejs3": "^7.12.5",
    "@godxiaoji/rollup-plugin-require-context": "^1.0.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^8.0.3",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.21",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-jest": "^26.6.3",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "^8.0.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^5.0.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "ora": "^5.3.0",
    "prettier": "^2.4.1",
    "rollup": "^2.59.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-sass": "^1.2.9",
    "rollup-plugin-svg-sprites": "^1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "1.32.6",
    "semantic-release": "^18.0.0",
    "through2": "^4.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vitepress": "0.18.1",
    "vue": "^3.2.21",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "sideEffects": [
    "src/**/style/*",
    "src/**/lib/*",
    "*.css",
    "*.scss",
    "*.vue"
  ]
}
